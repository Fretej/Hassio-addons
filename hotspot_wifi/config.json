{
  "name": "Hotspot wifi pour RPI",
  "version": "dev",
  "slug": "hotspot_wifi",
  "description": "Addon Hotspot wifi with raspap-webgui",
  "url": "https://github.com/ArminasTV/Hassio-addons/tree/master/hotspot_wifi",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "startup": "application",
  "arch": ["armv7", "aarch64"],
  "boot": "manual",
  "host_network": true,
  "privileged": ["NET_ADMIN", "SYS_ADMIN"],
  "devices": ["/dev/net/tun", "/dev/null", "/dev/urandom"],
  "map": ["config:rw", "share:rw"],
  "ports": {
    "80/tcp": 80
  },
  "ports_description": {
    "80/tcp": "Web interface"
  },
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "full_access": true,
  "options": {},
  "schema": {}
}
